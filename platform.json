{
  "name": "P51",
  "title": "Platform P51",
  "description": "Espressif wifi SoC",
  "homepage": "https://www.OS-Q.com/",
  "license": "Apache-2.0",
  "version": "2.6.4",
  "engines": {
    "platformio": "^5"
  },
  "keywords": [
    "dev-platform",
    "OS-Q",
    "Wi-Fi",
    "Xtensa",
    "Arduino"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/OS-Q/P51.git"
  },
  "frameworks": {
    "arduino": {
      "package": "A51",
      "script": "builder/frameworks/arduino.py"
    },
    "simba": {
      "package": "simba",
      "script": "builder/frameworks/simba.py"
    },
    "freertos": {
      "package": "F51",
      "script": "builder/frameworks/freertos.py",
      "title": "ESP8266 RTOS SDK"
    },
    "native": {
      "package": "N51",
      "script": "builder/frameworks/native.py",
      "description": "The non-OS SDK provides a set of application programming interfaces (APIs) for core ESP8266 functionalities",
      "homepage": "https://www.OS-Q.com/N51",
      "title": "ESP8266 SDK"
    }
  },
  "packages": {
    "A51": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/A51.git"
    },
    "F51": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/F51.git"
    },
    "N51": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/N51.git"
    },
    "simba": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/simba.git"
    },
    "toolchain-xtensa": {
      "type": "toolchain",
      "owner": "platformio",
      "version": "~2.40802.0",
      "optionalVersions": ["~1.40802.0"]
    },
    "tool-esptool": {
      "type": "uploader",
      "owner": "platformio",
      "version": "<2"
    },
    "tool-esptoolpy": {
      "type": "uploader",
      "owner": "platformio",
      "version": "~1.30000.0"
    },
    "tool-mkspiffs": {
      "type": "uploader",
      "optional": true,
      "owner": "platformio",
      "version": "~1.200.0"
    },
    "tool-mklittlefs": {
      "type": "uploader",
      "optional": true,
      "owner": "platformio",
      "version": "~1.203.0"
    }
  }
}
