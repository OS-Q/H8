{
  "name": "P51",
  "title": "OS-Q Platform ESP8266",
  "description": "Espressif8266 WiFi SoC with 160MHz LX106 Core",
  "homepage": "https://www.OS-Q.com/",
  "license": "Apache-2.0",
  "keywords": [
    "dev-platform",
    "Wi-Fi",
    "Xtensa",
    "106Micro"
  ],
  "engines": {
    "platformio": "^5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OS-Q/P51.git"
  },
  "version": "2.6.4",
  "frameworks": {
    "arduino": {
      "package": "A51",
      "script": "extend/frameworks/arduino.py"
    },
    "simba": {
      "package": "framework-simba",
      "script": "extend/frameworks/simba.py"
    },
    "esp8266-rtos-sdk": {
      "package": "E51B",
      "script": "extend/frameworks/esp8266-rtos-sdk.py"
    },
    "esp8266-nonos-sdk": {
      "package": "E51A",
      "script": "extend/frameworks/esp8266-nonos-sdk.py"
    }
  },
  "packages": {
    "toolchain-xtensa": {
      "type": "toolchain",
      "owner": "platformio",
      "version": "~2.40802.0",
      "optionalVersions": ["~1.40802.0"]
    },
    "A51": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/A51.git"
    },
    "E51B": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/E51B.git"
    },
    "E51A": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/E51A.git"
    },
    "framework-simba": {
      "type": "framework",
      "optional": true,
      "owner": "platformio",
      "version": ">=7.0.0"
    },
    "tool-esptool": {
      "type": "uploader",
      "owner": "platformio",
      "version": "<2"
    },
    "tool-esptoolpy": {
      "type": "uploader",
      "owner": "platformio",
      "version": "~1.30000.0"
    },
    "tool-mkspiffs": {
      "type": "uploader",
      "optional": true,
      "owner": "platformio",
      "version": "~1.200.0"
    },
    "tool-mklittlefs": {
      "type": "uploader",
      "optional": true,
      "owner": "platformio",
      "version": "~1.203.0"
    }
  }
}
